{
  "name": "projetoapifinal",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "node -r dotenv/config node_modules/mocha/bin/mocha 'test/**/*.test.js' --timeout 10000",
    "test-external": "node -r dotenv/config node_modules/mocha/bin/mocha 'test/external/**/*.test.js' --timeout 10000 --exit",
    "test-controller": "node -r dotenv/config node_modules/mocha/bin/mocha 'test/controller/**/*.test.js' --timeout 10000",
    "test-graphql": "node -r dotenv/config node_modules/mocha/bin/mocha 'test/graphql/**/*.test.js' --timeout 10000",
    "test-parallel": "concurrently --kill-others \"npm run start\" \"sleep 5 && node -r dotenv/config node_modules/mocha/bin/mocha test/external/user.test.js --timeout 10000\" \"sleep 5 && node -r dotenv/config node_modules/mocha/bin/mocha test/external/car.test.js --timeout 10000\" \"sleep 5 && node -r dotenv/config node_modules/mocha/bin/mocha test/external/rental.test.js --timeout 10000\" \"sleep 5 && node -r dotenv/config node_modules/mocha/bin/mocha test/graphql/graphql.test.js --timeout 10000\"",
    "start": "node src/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "apollo-server-express": "^3.13.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "graphql": "^15.10.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^3.4.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "apollo-server-testing": "^2.25.3",
    "chai": "^4.3.4",
    "concurrently": "^9.2.1",
    "jsonwebtoken": "^9.0.2",
    "mocha": "^11.7.2",
    "sinon": "^21.0.0",
    "supertest": "^7.1.4"
  }
}
